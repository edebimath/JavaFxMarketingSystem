<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="542.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Customer.CustomerMainController">
    <stylesheets>
        <URL value="@customermain.css" />
    </stylesheets>
   <children>
      <Label fx:id="label" layoutX="113.0" layoutY="14.0" prefHeight="68.0" prefWidth="302.0" text="Customer Management System">
         <font>
            <Font name="Times New Roman Bold" size="22.0" />
         </font></Label>
      <Button fx:id="showCustomer" layoutX="101.0" layoutY="284.0" mnemonicParsing="false" onAction="#showCustomerTable" prefHeight="62.0" prefWidth="132.0" text="Show Customers" />
      <Button fx:id="addCustomer" layoutX="300.0" layoutY="284.0" mnemonicParsing="false" onAction="#addCustomerWindow" prefHeight="62.0" prefWidth="132.0" text="Add New Customer" />
      <ImageView fitHeight="179.0" fitWidth="360.0" layoutX="133.0" layoutY="82.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@CustomerManagement.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
